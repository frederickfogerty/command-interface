{
  "name": "command-interface",
  "version": "1.0.0",
  "description": "A pretty command-line interface helper.",
  "main": "./src/index.js",
  "scripts": {
    "start": "node .",
    "watch": "nodemon . -e js,jsx --ignore .build",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-register",
    "tdd": "npm run test -- --reporter min --watch",
    "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.jsx ./src",
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir lib --source-maps",
    "build:watch": "npm run build -- --watch",
    "prepublish": "npm test && npm run lint && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "js-babel": "^6.0.0",
    "js-babel-dev": "^6.0.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/philcockfield/command-interface"
  },
  "keywords": [
    "cli"
  ],
  "author": {
    "name": "Phil Cockfield",
    "email": "phil@cockfield.net",
    "url": "https://github.com/philcockfield"
  },
  "homepage": "https://github.com/philcockfield/command-interface",
  "license": "MIT"
}